{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[pip-requirements]": {},
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "autoDocstring.startOnNewLine": true,
  "autopep8.importStrategy": "fromEnvironment",
  "black-formatter.cwd": "${fileDirname}",
  "black-formatter.importStrategy": "fromEnvironment",
  "continue.enableTabAutocomplete": false,
  "continue.showInlineTip": false,
  "continue.telemetryEnabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.autoClosingQuotes": "never",
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 8,
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.overtypeOnPaste": false,
  "editor.rulers": [120],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "visible",
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "[Gruvbox Dark Hard]": {
      "enabled": true,
      "rules": {
        "*.typeHint": "#fabd2f",
        "property.declaration:python": "#83A598",
        "property:python": "#83A598",
        "selfParameter": "#638779",
        "variable": {
          "foreground": "#83A598"
        }
      }
    }
  },
  "editor.showFoldingControls": "never",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.unicodeHighlight.allowedLocales": {
    "ru": true
  },
  "editor.unicodeHighlight.nonBasicASCII": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "files.associations": {
    "*.rmd": "markdown"
  },
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "never",
  "git.replaceTagsWhenPull": true,
  "git.suggestSmartCommit": false,
  "github.copilot.advanced": {
    "authProvider": "github"
  },
  "gitlens.codeLens.authors.enabled": false,
  "go.diagnostic.vulncheck": "Imports",
  "go.toolsManagement.autoUpdate": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "launch": {},
  "notebook.lineNumbers": "on",
  "python.analysis.extraPaths": [],
  "python.analysis.typeCheckingMode": "basic",
  "remote.autoForwardPortsSource": "hybrid",
  "security.allowedUNCHosts": ["wsl.localhost"],
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "update.showReleaseNotes": false,
  "vim.neovimConfigPath": "\\\\wsl$\\Ubuntu\\home\\anru\\.config\\nvim",
  "vim.neovimPath": "\\\\wsl$\\Ubuntu\\usr\\bin\\nvim",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
  "vscode-neovim.useWSL": true,
  "window.menuBarVisibility": "compact",
  "window.newWindowDimensions": "maximized",
  "window.restoreFullscreen": true,
  "window.zoomLevel": 1.2,
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.editor.restoreViewState": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 16,
  "zenMode.hideLineNumbers": false,
  "zig.zls.enabled": "on"
}
